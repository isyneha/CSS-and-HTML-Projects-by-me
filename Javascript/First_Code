    <script>
        function convertCtoF(temp){
            
           var temp;
            temp=((temp*9)/5 )+32;
            return temp;
        }
        var temp;
        temp=45;
        console.log("The temperature "+ temp +" degree Celsius is equal to " + convertCtoF(45) + " degree in Fahrenheit");
    </script>
